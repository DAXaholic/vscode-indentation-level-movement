{
    "name": "linejump",
    "displayName": "linejump",
    "description": "Effective line navigation",
    "version": "0.0.1",
    "publisher": "kaiwood",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.moveDown",
        "onCommand:extension.moveUp"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.moveDown",
            "title": "Semantic Down"
        }, {
            "command": "extension.moveUp",
            "title": "Semantic Up"
        }],
        "keybindings": [{
            "command": "extension.moveDown",
            "key": "ctrl-down",
            "mac": "alt-down",
            "when": "editorTextFocus"
        }, {
            "command": "extension.moveUp",
            "key": "ctrl-up",
            "mac": "alt-up",
            "when": "editorTextFocus"

        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}